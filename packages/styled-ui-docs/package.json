{
  "name": "styled-ui-docs",
  "version": "0.3.0",
  "private": true,
  "scripts": {
    "build": "cross-env NODE_ENV=production next build",
    "clean": "del build docs",
    "dev": "next dev --hostname 0.0.0.0",
    "postbuild": "next export --outdir docs && touch docs/.nojekyll",
    "pre-push": "eslint --ext .js,.jsx .",
    "start": "next start",
    "test": "jest --passWithNoTests"
  },
  "devDependencies": {
    "@emotion/core": "10.x",
    "@emotion/styled": "10.x",
    "@fortawesome/fontawesome-svg-core": "^1.2.27",
    "@fortawesome/free-brands-svg-icons": "^5.12.1",
    "@fortawesome/free-regular-svg-icons": "^5.12.1",
    "@fortawesome/free-solid-svg-icons": "^5.12.1",
    "@fortawesome/react-fontawesome": "^0.1.9",
    "@mdx-js/loader": "^1.5.7",
    "@mdx-js/mdx": "^1.5.7",
    "@mdx-js/react": "^1.5.7",
    "@next/mdx": "^9.3.0",
    "@trendmicro/react-styled-ui": "^0.3.0",
    "@trendmicro/styled-ui-theme": "^0.2.0",
    "@zeit/next-css": "^1.0.1",
    "boolean": "^3.0.1",
    "cross-env": "~7.0.2",
    "del-cli": "~3.0.0",
    "dotenv-flow": "~3.1.0",
    "emotion-theming": "10.x",
    "jest": "~25.1.0",
    "next": "9.3.2",
    "next-compose-plugins": "^2.2.0",
    "prism-react-renderer": "^1.0.2",
    "prop-types": "^15.6.0",
    "react": "^16.8.0",
    "react-custom-scrollbars": "^4.2.1",
    "react-dom": "^16.8.0",
    "react-live": "^2.2.2",
    "react-lorem-component": "^0.13.0",
    "react-table": "^7.0.4",
    "react-virtualized": "^9.21.2",
    "remark-emoji": "^2.0.2",
    "remark-images": "^1.0.0"
  },
  "jest": {
    "testMatch": [
      "<rootDir>/test/**/*.js"
    ],
    "collectCoverageFrom": [
      "<rootDir>/components/**/*.{js,jsx}",
      "<rootDir>/pages/**/*.{js,jsx}",
      "<rootDir>/shared/**/*.js"
    ],
    "coverageReporters": [
      "lcov",
      "text",
      "html"
    ]
  }
}
